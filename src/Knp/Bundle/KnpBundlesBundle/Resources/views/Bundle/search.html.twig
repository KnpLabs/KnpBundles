{% extends 'KnpBundlesBundle::layout/900px.html.twig' %}

{% block title %}{% trans %}search.list.meta.title{% endtrans %}{% endblock %}

{% block description %}{% trans %}search.list.meta.description{% endtrans %}{% endblock %}

{% block intro %}
<div id="intro">
    <h1>{{ block('title') }}</h1>
</div>
{% endblock %}

{% block left %}
<div class="content-box">
    <hgroup>
        <h2>{% trans %}search.list.title{% endtrans %}</h2>
    </hgroup>

    <section>
        <p>{% trans %}search.content.description.part1{% endtrans %}<br />
        {% trans %}search.content.description.part2{% endtrans %}</p>
    </section>
</div>
{% endblock %}
